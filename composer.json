{
    "name": "wxsd/early-one-button-to-push",
    "type": "project",
    "description": "Customize the timing of OBTP on your shared Webex devices.",
    "keywords": ["webex", "azure", "devices", "xapi", "obtp", "meeting"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.1",
        "ext-json": "*",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4.1",
        "laravel/framework": "^8.82",
        "laravel/sanctum": "^2.14",
        "laravel/telescope": "^4.7",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "socialiteproviders/microsoft-azure": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.17",
        "fakerphp/faker": "^1.18.0",
        "laravel/sail": "^1.13.1",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^5.11",
        "phpunit/phpunit": "^9.5.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
